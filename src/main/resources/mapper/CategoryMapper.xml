<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.famigo.backend.mapper.CategoryMapper">

  <!-- カテゴリ一覧取得（categories に論理削除カラムは無い） -->
  <select id="selectAll" resultType="com.famigo.backend.dto.CategoryDto">
    SELECT
    id,
    name
    FROM categories
    ORDER BY id
  </select>

</mapper>